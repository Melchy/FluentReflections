<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netstandard2.1</TargetFramework>
        <Nullable>enable</Nullable>
        <IncludeSymbols>true</IncludeSymbols>
        <SymbolPackageFormat>snupkg</SymbolPackageFormat>
        <PackageId>FluentReflections</PackageId>
        <Authors>Michal Motyčka</Authors>
        <Product>FluentReflections</Product>
        <Company>Michal Motyčka</Company>
        <Description>Fluent reflections.</Description>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <PackageProjectUrl>https://github.com/Melchy/FluentReflections</PackageProjectUrl>
        <RepositoryUrl>https://github.com/Melchy/FluentReflections</RepositoryUrl>
        <PackageTags>Reflection, fluent</PackageTags>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="Microsoft.CodeAnalysis.FxCopAnalyzers" Version="2.9.8" />
        <PackageReference Include="Microsoft.Extensions.DependencyModel" Version="3.0.1" />
    </ItemGroup>
</Project>
